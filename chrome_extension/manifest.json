{
  "name": "Math Agent Calculator",
  "version": "1.0",
  "description": "Solve complex math problems step by step",
  "manifest_version": 3,
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icon16.png",
      "48": "icon48.png",
      "128": "icon128.png"
    }
  },
  "permissions": [
    "storage"
  ],
  "host_permissions": [
    "http://localhost:5000/*"
  ]
}